config interface 'lan'
        option type 'bridge'
        option proto 'static'
        option netmask '255.255.255.0'
        option ip6assign '60'
        option delegate '0'
        option _orig_ifname 'eth0.1 ra0 rax0'
        option _orig_bridge 'true'
        option ifname 'eth0.1 rax0'
        option ipaddr '192.168.2.1'
